{
  "author": "igorlogius",
  "background": {
	"scripts": [ "background.js" ]
  },
  "browser_action": {
	  "default_title": "Get Alexa Info",
	  "default_popup": "popup.html"
  },
  "icons": {
	"128": "icon.png"
  },
  "description": "display alexa information",
  "manifest_version": 2,
  "name": "get-alexa-info",
  "permissions": [
    "activeTab",
    "https://www.alexa.com/minisiteinfo/*"
  ],
  "optional_permissions": [
    "webNavigation"
  ],
  "sidebar_action": {
	 "default_panel": "sidebar.html",
	 "open_at_install": false,
	 "default_icon": "icon.png",
	 "default_title": "Get Alexa Info"
  },
  "version": "1.8.15"
}
